journal = {International Journal of Computer Assisted Radiology and Surgery},
keywords = {B-spline snake,Boundary detection,Echocardiography,Left ventricle volume estimation},
mendeley-groups = {AAM,分割心室综述},
number = {5},
pages = {501--513},
pmid = {20232263},
title = {{Endocardial boundary extraction in left ventricular echocardiographic images using fast and adaptive B-spline snake algorithm}},
volume = {5},
